	thermal-zones {
		cpu-thermal-zone {
			sustainable-power = <1600>;

			trips {
				fan_config0: fan-trip0 {
					temperature = <40000>;
					hysteresis = <5000>;
					type = "active";
				};

				fan_config1: fan-trip1 {
					temperature = <50000>;
					hysteresis = <5000>;
					type = "active";
				};

				fan_config2: fan-trip2 {
					temperature = <60000>;
					hysteresis = <5000>;
					type = "active";
				};
			};

			cooling-maps {
				fan-on {
					trip = <&fan_config0>;
					cooling-device =
						<&fan 1 1>;
				};
				fan-faster {
					trip = <&fan_config1>;
					cooling-device =
						<&fan 2 2>;
				};
				fan-full {
					trip = <&fan_config2>;
					cooling-device =
						<&fan 3 3>;
				};
			};
		};

		dev-thermal-zone {
			sustainable-power = <3000>;
		};
	};

