		reg = <0x08>;
		sound-name-prefix = "ES8156";
		AVDD-supply = <&soc_aud_3v3_en_reg>;
		DVDD-supply = <&soc_aud_1v8_en_reg>;
		PVDD-supply = <&soc_aud_1v8_en_reg>;
	};

